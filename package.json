{
  "name": "angular2-mdl-seed",
  "version": "0.0.1-alpha.1",
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" \"npm run watch-css\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "build-css": "node-sass assets/css/main.scss assets/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.16",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "material-design-lite": "^1.1.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.26",
    "tslint": "^3.4.0",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
